			.wrap
				%article.entry.h-entry
					%header.entry-head.cf
						.entry-id
							%h1.entry-title.p-name Grand Hyatt Barra da Tijuca Residences
							%p.entry-category.p-category<
								%a{:href => "#"}> Apartamento
								,&nbsp;
								%a{:href => "#"}> Suítes Residenciais na Barra da Tijuca
